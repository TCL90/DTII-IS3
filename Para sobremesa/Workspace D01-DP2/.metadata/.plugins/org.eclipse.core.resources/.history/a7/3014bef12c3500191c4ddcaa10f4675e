		final ModelAndView res;
		final Collection<Member> members;
		
		final Brotherhood b=this.brotherhoodService.findOnePrincipal();
		this.memberService.membersByBrotherhood()
		res = new ModelAndView("/list");
		res.addObject("applications", applications);
		res.addObject("vat", VAT);
